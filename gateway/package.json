{
  "name": "foxiny-gateway",
  "version": "0.1.0",
  "description": "foxiny gateway",
  "author": "foxiny-dev",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "schema": "graphql get-schema --project foxiny-gateway --dotenv ../config/dev.env",
    "dev": "env-cmd ../config/dev.env nodemon ./src/index.js --exec babel-node ./src/index.js --ext js,graphql --experimental-worker",
    "debug": "env-cmd ../config/dev.env babel-node ./src/index.js",
    "test": "env-cmd ../config/test.env jest --watchAll --runInBand",
    "prod": "env-cmd ../config/prod.env node ./dist/index.js",
    "postbuild": "babel src --out-dir dist --copy-files",
    "start": "node ./dist/index.js",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "jest": {
    "globalSetup": "./tests/jest/start-test-server.js",
    "globalTeardown": "./tests/jest/stop-test-server.js",
    "collectCoverage": true,
    "moduleDirectories": [
      "node_modules",
      "./tests/data"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "svg"
    ]
  },
  "lingui": {
    "fallbackLocale": "",
    "sourceLocale": "",
    "localeDir": "<rootDir>/locale",
    "srcPathDirs": [
      "<rootDir>"
    ],
    "srcPathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/src/resolvers/type-defs/"
    ],
    "format": "lingui"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "accept-language-parser": "^1.5.0",
    "apollo-server-express": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "crypto-random-string": "^1.0.0",
    "express": "^4.16.4",
    "express-request-language": "^1.1.15",
    "graphql-binding": "^2.4.1",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "ms": "^2.1.1",
    "prisma-binding": "^2.2.8",
    "redis": "^2.8.0",
    "request-ip": "^2.1.3",
    "twilio": "^3.26.1",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@lingui/babel-plugin-extract-messages": "^2.8.3",
    "@lingui/babel-plugin-transform-js": "^2.8.3",
    "@lingui/babel-preset-js": "^2.8.3",
    "@lingui/cli": "^2.8.3",
    "@lingui/core": "^2.8.3",
    "@lingui/macro": "^2.8.3",
    "apollo-boost": "^0.1.23",
    "aws-sdk": "^2.413.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-macros": "^2.6.0",
    "cross-fetch": "^2.2.3",
    "cuid": "^2.1.6",
    "env-cmd": "^8.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.86.0",
    "graphql": "^14.4.2",
    "graphql-cli": "^3.0.3",
    "graphql-type-json": "^0.3.0",
    "jest": "^23.6.0",
    "lingui-cli": "^1.4.5",
    "lodash": "^4.17.11",
    "ngrok": "^3.1.0",
    "nodemon": "^1.18.6",
    "subscriptions-transport-ws": "^0.9.16"
  }
}
